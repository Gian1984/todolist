eturn{\n            title:'',\n        }\n    },\n    methods: {\n        addTodo(){\n            const NewTodo =\n            {\n                id: uuid.V4, //calling the fake id \n                title: this.title, // this will recover the title from the vmodel=\"title\" on the form\n                completed:false // whenever we add a new todo it's ever false \n            }\n\n        //send up to parents\n        $this.$emit('add-Todo',NewTodo)\n        }\n    }\n}\n</script>\n\n<style scoped>\n    form {\n        display: flex;\n    }\n    input[type=\"text\"] {\n        flex: 10;\n        padding: 5px;\n    }\n    input[type=\"submit\"] {\n        flex: 2;\n    }\n</style>"],"sourceRoot":"src/components"}]}